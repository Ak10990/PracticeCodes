import java.util.*;

class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner s = new Scanner(System.in);
        int T = s.nextInt(),n,i,j,k,m1,m2;String str,ans;char x;

        for (i = 0; i < T; i++) {
            n=s.nextInt();
            m1=-1;m2=-1;
            ans="YES";
            for(j=0;j<n;j++){
                str=s.next();
                if(ans.equals("YES")){
                for(k=0;k<str.length();k++) {
                   x= str.charAt(k);
                   if(x=='#') {
                        if(m1==-1)
                            {m1=k;}else if(m2!=-1&&k<m1){
                                ans="NO";
                                break;
                            }else if(m2!=-1){
                                
                            }else if(m2==-1){
                                
                            }else{}
                   }else {
                       if(m1!=-1){
                       if(m2==-1){
                           m2=k;
                       }else if(k>=m1&&k<=m2){
                           ans="NO1";
                           break;
                       }
                       }
                        
                   }
                }
                if(m1!=-1 &&  m2==-1){
                    m2=m1;
                }
            }
            }
            System.out.println("Case #"+(i+1)+": "+ans+"    "+m1+"  "+m2);
        }
    }
}

